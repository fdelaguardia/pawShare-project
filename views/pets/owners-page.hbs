{{> navBar}}

<h1>{{foundPets.0.owner.firstName}}'s Page</h1>


<div class="outside-div">
    {{#each foundPets}}
       
        <div class="div-border">
            <img class="icon-img" src="{{iconImage}}" alt="icon-image">
            <p>{{name}}</p>
            <a href="/home/pet-page/{{_id}}" class="home-btns">See pet page</a>
            <hr>
        </div>
       
    {{/each}}
</div>